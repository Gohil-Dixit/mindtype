    "zod": "^3.24.2",
    "memorystore": "^1.6.7",
    "multer": "^2.0.2",
    "connect-pg-simple": "^10.0.0",
    "express-session": "^1.18.1",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "esbuild": "^0.25.0",
    "tsx": "^4.20.5",
    "vite": "^5.4.20",
    "@types/express": "4.17.21",
    "@types/node": "20.16.11",
    "typescript": "5.6.3"
  }
